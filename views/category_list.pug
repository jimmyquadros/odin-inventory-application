extends layout 

block content 
  h1= title 

  a(href='/category/create') New Category
  br
  br
  ul.list-grid.category-list
    li
      div NAME 
    li 
      div QTY
    each category in category_list 
      li 
        a(href=category[0].url) #{category[0].name}
      li
        div #{category[1]}
    else 
      li No Categories